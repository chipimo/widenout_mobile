{
  "expo": {
    "name": "WidenOut",
    "slug": "widenout",
    "version": "4.7.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from WidenOut"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/ca36d396-e5cb-45ac-be5d-879e1ecf5ffb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/ios_icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.theps.widenout",
      "buildNumber": "7.0.0"
    },
    "android": {
      "icon": "./assets/ios_icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.theps.widenout",
      "versionCode": 7
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ca36d396-e5cb-45ac-be5d-879e1ecf5ffb"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
